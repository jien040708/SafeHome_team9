[run]
source = .
omit =
    */tests/*
    */test_*.py
    */__pycache__/*
    */virtual_device*/*
    main.py
    ui/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    class .*\(Protocol\):
    def .*\(.*\).*\.\.\..*:$

[html]
directory = htmlcov

